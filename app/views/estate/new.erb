<% if @rent %>
  <%= form_for :rent, @rent, :url => { :action => "create"} do |f| %>
    город : <%= select_tag "city", options_for_select([["Киев"]]) %>
    район :    <%= collection_select :rent, :dist_code, Dist.all, :dist_code, :name_initial ,{:class => "styled"} %><br>
    адрес :<%= f.autocomplete_field :street, :rus_name, autocomplete_street_rus_name %><br>
        <hr>

    <%= select_tag "rooms", options_for_select(
        [["1 комнатная", 1],
         ["2 комнатная",2],
         ["3 комнатная",3],
         ["4 комнатная",4] ,
         ["5 комнатная",5],
         ["6 комнатная",6]]),
               :id => "rooms_num", :class => "styled", :name => "rent[rooms]"  %><br>

    площадь всего :<%= f.text_field :total_sqr  %>
    жилая площадь :<%= f.text_field :live_sqr  %>
    площадь кухни :<%= f.text_field :kitchen_sqr  %><br>

    этаж :<%= f.text_field :floor_at  %><br>
    этажность :<%= f.text_field :floors  %><br>
    тип стен :<%= select_tag("rent[wall_type]", options_for_select([["кирпич"], ["панель"], ["блок"]]))  %><br>
    детали :<%= f.text_field :info  %><br>
    имя :<%= f.text_field :name  %><br>
    телефоны :<%= f.text_field :phones  %><br>
    цена :<%= f.text_field :price  %><br>
    <%= submit_tag "add" %>
  <% end %>

<% end %>

<% content_for :head do %>
  <%= javascript_include_tag "autocomplete-rails.js" %>
<% end %>
<!--collection_select(object, method, collection, value_method, text_method, options = {}, html_options = {})-->

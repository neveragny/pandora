<% if @rent %>
  <%= form_for @rent, :url => new_estate_path(@rent) do |f| %>
    <%= f.text_field :adress  %>
    <%= f.text_field :price  %>
    <%= f.text_field :rooms  %>
    <%= f.text_field :floor_at  %>
    <%= f.text_field :floors  %>
    <%= f.text_field :live_sqr  %>
    <%= f.text_field :kitchen_sqr  %>
    <%= f.text_field :total_sqr  %>
    <%= f.text_field :wall_type  %>
    <%= f.text_field :info  %>
    <%= f.text_field :name  %>
    <%= f.text_field :phones  %>
    <%= f.text_field :dist_code  %>
  <% end %>

<% end %>  

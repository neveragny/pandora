<% @rents.each do |item| %>
    <tr data-longitude="<%= item.longitude %>" data-latitude="<%= item.latitude %>" data-id="<%= item %>" class="listingRow" id="listing_383659_row1">
      <td class="border thumbnail">

        <!--<a href="http://www.nakedapartments.com/rental/383659" class=" ">-->
        <%= link_to item do %>
          <!--<img src="http://s3.amazonaws.com/nakedapartments/images/4505761/thumb.jpg?1309203580" alt="Thumb">-->
          <% if item.rentphotos.blank?  %>
            <%= image_tag '/images/no_available.png' %>
          <% else %>
			<%= image_tag item.rentphotos[0].photo.url(:thumb) %>
          <% end %>
        <% end %>
      </td>

      <td class="border flag">

        <span class="listingTitle">
          <%= link_to item do %>
            <%= item.rooms %>к, <%= item.adress %> 
			<div><%= item.price > 0 ? "#{item.price} грн" : "договорная"  %></div>
          <% end %>
        </span>

        <div class="newCell listingDate">
          <%= time_ago(item.updated_at) %>
          <span class="calm"><%= t(:onsite) %></span>
        </div>


        <div class="newCell brokerCont">

          <a href="#userpath"><%= item.name %></a>
          
          <%# if favorited? item  %>
<!--          <span class="isFavorited">
            <span><%#= raw('&nbsp;') %></span>
          </span> -->
          <%# else %>
          <span class="">
          <%= link_to_favorite item %>

          </span>
          <%# end %>
        </div>


        <div data-id="<%= item %>" class="">
          <div class="newCell amenities">

            <!--Doorman, Elevator, Laundry Room, Pets Allowed-->
            <%= item.info %>

          </div>
        </div>


        <div class="hidden mapMarkerHtml">
          <div class="center">
            <img alt="" src="http://s3.amazonaws.com/nakedapartments/images/4505761/thumb.jpg?1309203580">
            <br>
            <%= link_to item do %>
                <%= item.rooms %>к, <%= item.adress %> <%= item.price %> грн
            <% end %>
          </div>
        </div>

      </td>
    </tr>

<% end %>


<html class=" gecko mac js native-css-gradients" xmlns:fb="http://www.facebook.com/2008/fbml">
<head xmlns:fb="http://www.facebook.com/2008/fbml" xmlns="http://www.w3.org/1999/xhtml">
  <meta name="MSSmartTagsPreventParsing" content="true" />
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=EmulateIE7" />
  <meta content="text/html; charset=utf-8" http-equiv="Content-Type"/>
  <title>Пандора - проект "Живая база недвижимости" Киев.</title>
  <link rel="icon" href="/images/favicon.ico" type="image/x-icon" />

  <%= stylesheet_link_tag 'reset.css' %>
  <%= javascript_include_tag 'jquery.js', 'rails.js', 'application.js', 'applesearch.js', 'jcookie.js'%>
  <%= javascript_include_tag 'form', 'jquery.remotipart' %>

  <%= yield :head %>
  <%= csrf_meta_tag %>

</head>
<body>
<body id="flow">
	<div id="slab-wrapper">
		<h1>
			<strong>Регистрация</strong>&nbsp;&mdash; это Просто.
		</h1>
		<p class="signin">
			уже с нами?
			<a id="nav_sign_in" href="/signin">войти</a>
		</p>
		<div id="slab">
          <%= render :partial => 'shared/errors', :locals => {:target => @user} %>

          <%= form_for @user do |f| -%>

              <div class="main">
                  <div class="field">
                    <%= f.label "логин" %><%= f.text_field :login %>
                  </div> 
                  <div class="field">
                    <%= f.label "почтовый ящик" %><%= f.text_field :email %>
                  </div>
                  <div class="field">
                    <%= f.label "пол" %>пацанчик<%= f.radio_button :sex, 1, :checked => true %>
                                         деваха<%= f.radio_button :sex, 0 %>
                  </div>  
                  <div class="field">
                    <%= f.label "пароль" %><%= f.password_field :password %>
                  </div> 
                  <div class="field">
                    <%= f.label "еще разок пароль" %><%= f.password_field :password_confirmation %>
                  </div>
              </div>
              <div class="submit">
                <%= f.submit t(:register), :class => "button", :disable_with => t(:please_wait) %>
                <!--<a class="button" href="#submit">Sign up</a>-->
              </div>
          <% end %>

        </div>
        <div class="clear"></div>
      </div>
    </div>
  </div>
  <div id="signup-footer"></div>

    <div class="modal_background" style="width: 100%; height: 100%;"></div>
    <div id="modal1" class="modal window" style="top: 43.125px; left: 355.5px;">
      <div id="login" class="wrapper">
        <div class="title">
            <h1>Log in</h1>
        </div>
        <%= form_for(UserSession.new) do |f| %>
          <div id="form">
            <fieldset>
              <label><%= f.label t(:user_login) %></label>
                <div><%= f.text_field :login %></div>
            </fieldset>
            <fieldset>
              <label><%= f.label t(:password) %></label>
                <div><%= f.password_field :password %></div>
            </fieldset>
            <fieldset>
                <label></label>
                <div class="forgot">
                    <a href="#">не помним пароль?</a>
                </div>
            </fieldset>
          </div>
          <div class="submit">
            <%= f.submit t(:login), :class=> :button, :disable_with => t(:please_wait) %>
            <%= link_to t(:register), register_path, :class => "signup" %>
            <a href="#cancel" class="cancel">Cancel</a>
          </div>
            <!--<fieldset>-->
                <%#= f.label t(:remember_me) %> <%#= f.check_box :remember_me %>
            <!--</fieldset>-->
        <% end %>




      </div>
    </div>


  </div>
	</div>

</body>
</html>



<style>
    html, body, div, span, applet, object, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, font, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td {
        border: 0 none;
        font-family: inherit;
        font-size: 100%;
        font-style: inherit;
        font-weight: inherit;
        margin: 0;
        outline: 0 none;
    }

    body {
        background: none repeat scroll 0 0 #E9E9E9;
        color: #333333;
        font-family: 'Helvetica Neue',Helvetica,Arial,sans-serif !important;
        font-size: 13px;
    }
    body, #content {
        background: none repeat scroll 0 0 #D3E5F4 !important;
        padding: 10px;
        min-height: 371px;
    }
    body #main, #content #main {
        background: none repeat scroll 0 0 #FFFFFF;
        margin: 0 10px;
        overflow: hidden;
        padding: 0;
        border: medium none !important;
        border-radius: 0 0 10px 10px !important;
    }
    body div#content div#wrapper {
        margin: 0 auto;
        width: 990px;
    }
    #signup {
        background: url("/images/bg_signup.png?1297106063") no-repeat scroll 0 0 #2E4457;
        overflow: hidden;
        padding: 30px 60px 30px 30px;
    }
    #signup > form {
        background: none repeat scroll 0 0 #627C92;
        border-radius: 30px 30px 30px 30px;
        color: #D3E5F4;
        float: right;
        overflow: hidden;
        padding: 30px;
        width: 550px;
    }
    #signup > form .title {
        margin-bottom: 10px;
        overflow: hidden;
    }
    #signup > form .title h2, #signup > form .title a {
        font-size: 20px;
        font-weight: bold;
        line-height: 24px;
    }
    #signup > form .title h2 {
        float: left;
    }
    #signup > form .title h2 span {
        font-size: 11px;
        font-weight: normal;
    }
    #signup > form .title a {
        color: inherit;
        float: right;
        text-decoration: underline;
    }
    #signup > form label {
        display: block;
        font-weight: bold;
        margin: 15px 0 5px;
    }
    #signup > form label.status {
        display: inline-block;
        font-size: 15px;
        max-width: 220px;
        padding-left: 10px;
    }
    #signup > form label.status.error {
        color: #EB9400;
    }
    #signup > form label.status.success {
        color: #B1D422;
    }
    #signup > form label img {
        margin-right: 5px;
        position: relative;
        top: -3px;
        vertical-align: middle;
    }
    #signup > form label .formError, #signup > form .fieldWithErrors {
        display: inline;
    }
    #signup > form .error {
        background: none repeat scroll 0 0 transparent;
    }
    #signup > form input[type="text"], #signup > form input[type="password"] {
        background: none repeat scroll 0 0 #FFFFFF;
        border: medium none;
        border-radius: 7px 7px 7px 7px;
        font-size: 20px;
        font-weight: bold;
        padding: 10px;
        width: 290px;
    }
    #signup > form .submit {
        margin-top: 18px;
        padding: 12px 0;
    }

    #signup > form .submit{
        font-size: 20px;
        font-weight: bold;
    }
    #signup > form .submit > a.more {
        color: inherit;
        float: right;
        padding: 10px 0;
        text-decoration: underline;
    }
    #signup > form .submit .button {
        background: none repeat scroll 0 0 #A3D62F;
        border: medium none;
        color: #FFFFFF;
        float: left;
        padding: 10px 30px;
    }
    #signup > form .submit > .button:hover {
        background: none repeat scroll 0 0 #9CC242;
    }
    #signup > form .submit p.terms {
        clear: both;
        font-size: 11px;
        padding-top: 20px;
    }
    #signup > form .submit p.terms a {
        color: inherit;
        text-decoration: underline;
    }
    #facebook-button {
        float: right;
        margin-top: 10px;
    }
    .formError {
        color: #FF8888;
    }

</style>


